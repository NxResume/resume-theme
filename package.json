{
  "name": "resume-theme",
  "type": "module",
  "version": "0.1.7",
  "packageManager": "pnpm@10.12.1",
  "description": "A collection of beautiful resume themes for NxResume",
  "author": "ryanuo <ryanuo@aliyun.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/ryanuo",
  "homepage": "https://github.com/NxResume/resume-theme#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NxResume/resume-theme.git"
  },
  "bugs": "https://github.com/NxResume/resume-theme/issues",
  "keywords": [
    "resume",
    "theme",
    "nxresume",
    "css",
    "scss"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./themes": {
      "default": "./dist/themes/index.css"
    },
    "./themes/*": {
      "default": "./dist/themes/*/style.css"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:dev",
    "@antfu/ni": "catalog:dev",
    "@antfu/utils": "catalog:dev",
    "@types/fs-extra": "catalog:dev",
    "@types/node": "catalog:dev",
    "bumpp": "catalog:dev",
    "eslint": "catalog:dev",
    "esno": "catalog:dev",
    "fs-extra": "catalog:dev",
    "lint-staged": "catalog:dev",
    "sass": "catalog:dev",
    "simple-git-hooks": "catalog:dev",
    "typescript": "catalog:dev",
    "unbuild": "catalog:dev",
    "vite": "catalog:dev",
    "vitest": "catalog:dev"
  },
  "overrides": {
    "jiti": "2.4.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "changelogithub": {
    "types": {
      "feat": {
        "title": "🚀 Features"
      },
      "fix": {
        "title": "🐞 Bug Fixes"
      },
      "perf": {
        "title": "🏎 Performance"
      },
      "refactor": {
        "title": "♻️ Refactors"
      },
      "docs": {
        "title": "📚 Documentation"
      },
      "build": {
        "title": "📦 Build"
      }
    }
  }
}
